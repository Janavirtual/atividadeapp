<p id="notice"><%= notice %></p>

<h1>Lista de Aeronaves</h1>

<table>
  <thead>
  <ul>
<li>Nome</li>
<li>Número de tripulantes</li>
<li>Tamanho</li>
<li>Número</li>
<li>Chassi</li>


  </thead>

  <tbody>
    <% @aeronaves.each do |aeronafe| %>
      <tr>
        <td><%= aeronafe.nome %></td>
        <td><%= aeronafe.numeroDeTripulantes %></td>
        <td><%= aeronafe.tamanho %></td>
        <td><%= aeronafe.numero %></td>
        <td><%= aeronafe.chassi %></td>
        <td><%= aeronafe.quantidadeDeHorasDevoo %></td>
        <td><%= aeronafe.quantidadeDeMotor %></td>
        <td><%= link_to 'Show', aeronafe %></td>
        <td><%= link_to 'Edit', edit_aeronafe_path(aeronafe) %></td>
        <td><%= link_to 'Destroy', aeronafe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

